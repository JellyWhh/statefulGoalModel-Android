<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.fudan.se"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- 连接网络的权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- 往sdcard中写入数据的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 在sdcard中创建/删除文件的权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name="edu.fudan.se.initial.SGMApplication"
        android:allowBackup="true"
        android:icon="@drawable/app__launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service android:name="jade.android.MicroRuntimeService" />

        <activity
            android:name="edu.fudan.se.initial.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".maincontainer.MainActivity" >
        </activity>
        <activity android:name=".goalmodeldetails.GoalModelDetailsActivity" >
        </activity>
        <activity android:name="edu.fudan.se.clientgui.SettingAgentActivity" >
        </activity>
        <activity android:name="edu.fudan.se.clientgui.StartAgentActivity" >
        </activity>
        <activity android:name="edu.fudan.se.clientgui.WorkingActivity" >
        </activity>
    </application>

</manifest>